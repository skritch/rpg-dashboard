		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"reconnecting-websocket": "^4.4.0",
		"rxjs": "^7.8.2",
		"uuid": "^11.1.0",
		"ws": "^8.18.3",
		"zod": "^4.0.17"
	}
}
